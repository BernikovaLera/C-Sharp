        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="42"/>
            <RowDefinition/>
            <RowDefinition Height="42"/>
        </Grid.RowDefinitions>
        <Rectangle Fill="#FFF4F4F5" Grid.Row="2" StrokeThickness="0"/>
        <Frame x:Name="MainFrame" Content="Начало" Grid.Row="1" Background="Beige" />
        <Button x:Name="BackButton" Content="Назад" HorizontalAlignment="Left" Margin="354,10,0,0" Width="75" Click="BackButton_Click"/>
        <Button x:Name="ForwardButton" Content="Вперёд" HorizontalAlignment="Left" Margin="434,10,0,0" Width="75" Click="ForwardButton_Click"/>
        <Button x:Name="Authorization" Content="Авторизация"  VerticalAlignment="Center" Margin="10,22,428.667,0" Click="Authorization_Click" />
        <Button x:Name="Registration" Content="Регистрация"  VerticalAlignment="Center" Margin="100,22,338.667,0" Click="Registration_Click"/>
    </Grid>
</Window>
